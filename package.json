{
  "name": "service-activator",
  "version": "1.1.0",
  "description": "A full stack app template for the G5 OpEx team.",
  "author": "David Miller & Tyler Hasenoehrl",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "docker:build": "docker build -t service-activator .",
    "helm:upgrade": "helm upgrade service-activator ./chart/nuxt",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "appmetrics-prometheus": "^3.1.0",
    "appmetrics-zipkin": "^1.1.1",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.16.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "greenlock": "^4.0.4",
    "jquery": "^3.5.1",
    "mysql2": "^2.1.0",
    "nuxt": "^2.14.4",
    "papaparse": "^5.3.0",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "sequelize": "^6.28.2",
    "validator": "^13.1.1",
    "vue-multiselect": "^2.1.6",
    "vue-multiselect-listbox": "0.0.20",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@vue/test-utils": "^1.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.3.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.9",
    "sass-loader": "^8.0.2",
    "vue-jest": "^3.0.6"
  }
}
